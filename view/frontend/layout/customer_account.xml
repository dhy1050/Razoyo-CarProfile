<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="customer_account_navigation">
            <block class="Magento\Framework\View\Element\Html\Link\Current" name="customer-account-navigation-car-profile" after="customer-account-navigation-wish-list-link">
                <arguments>
                    <argument name="path" xsi:type="string">carprofile</argument>
                    <argument name="label" xsi:type="string">My Car</argument>
                    <argument name="sortOrder" xsi:type="number">215</argument>
                </arguments>
            </block>
        </referenceContainer>
    </body>
    <move element="customer-account-navigation-car-profile" destination="customer_account_navigation" after="customer-account-navigation-wish-list-link"/>
</page>
